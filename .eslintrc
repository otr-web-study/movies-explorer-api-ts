{
  "parser": "@typescript-eslint/parser",
  "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true
  },
  "extends": ["airbnb-base", "plugin: @typescript-eslint/recommended"],
  "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
  },
  "rules": {
    "no-console":0,
    "no-underscore-dangle": ["error", { "allow": ["_id"] }]
  }
}
